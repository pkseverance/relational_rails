<%= form_with url: '/players', method: :post, local: true do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name %>
    
    <%= form.label :age %>
    <%= form.text_field :age %>

    <%= form.label :is_online %>
    <%= form.text_field :is_online %>

    <%= form.submit 'Create Player' %>
<% end %>